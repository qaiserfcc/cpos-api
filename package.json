{
  "name": "cpos-api",
  "version": "1.0.0",
  "description": "Cloud Point of Sale API backend",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "build": "npm run migrate && npm run seed && npm run build:app",
    "build:app": "echo 'Build completed'",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "postbuild": "npm run migrate:deploy && npm run seed:deploy",
    "migrate:deploy": "node scripts/migrate.js",
    "seed:deploy": "node scripts/seed.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "keywords": [
    "pos",
    "api",
    "express",
    "postgresql",
    "prisma"
  ],
  "author": "CPOS Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.22.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
